
CREATE DATABASE clg_mgmt;
use clg_mgmt;
CREATE TABLE dept_details (
dept_id INT NOT NULL PRIMARY KEY,
dept_name VARCHAR(60) NOT NULL,
hod_name VARCHAR(60),
placement_officer VARCHAR(15),
placementofficre_no VARCHAR(15)

);
