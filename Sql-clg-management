CREATE TABLE placed_students (
placement_id INT NOT NULL PRIMARY KEY,
s_id INT NOT NULL,
company_name VARCHAR(100),
package FLOAT,
FOREIGN KEY (s_id) REFERENCES s_details(s_id)
);
