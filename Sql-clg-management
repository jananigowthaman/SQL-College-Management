CREATE TABLE l_details (
s_id INT NOT NULL PRIMARY KEY,
book_id INT NOT NULL,
no_of_book_have INT NOT NULL,
due_amount INT,
FOREIGN KEY (s_id) REFERENCES s_details(s_id)
);
