SELECT s.s_id, s.s_name,d.dept_name,s.hostal_or_daysc,t.route, t.driver_name,
ps.company_name, ps.package
FROM s_details s
LEFT JOIN dept_details d ON s.dept_id = d.dept_id
LEFT JOIN transport_details t ON s.transport_id = t.transport_id
LEFT JOIN placed_students ps ON s.s_id = ps.s_id;
